@Article{bowey_phonological_2005,
  Title                    = {Phonological recoding and rapid orthographic learning in third-graders’ silent reading: A critical test of the self-teaching hypothesis},
  Author                   = {Bowey, Judith A. and Muller, David},
  Journal                  = {Journal of Experimental Child Psychology},
