@Article{muller_dietary_2009,
  Title                    = {Dietary Patterns and Prostate Cancer Risk},
  Author                   = {Muller, David C. and Severi, Gianluca and Baglietto, Laura and Krishnan, Kavitha and English, Dallas R. and Hopper, John L. and Giles, Graham G.},
  Journal                  = {Cancer Epidemiology Biomarkers \& Prevention},
  Year                     = {2009},

  Month                    = nov,
  Number                   = {11},
  Pages                    = {3126--3129},
  Volume                   = {18},

  Abstract                 = {Recent studies report that certain dietary patterns, especially those high in red and processed meats, are associated with prostate cancer risk. We prospectively investigated associations between empirically derived dietary patterns and prostate cancer risk using the Melbourne Collaborative Cohort Study. We followed 14,627 men of ages 34 to 75 years for an average of 13.6 years, and identified 1,018 incident prostate cancers. Factor analysis of the 121-item food frequency questionnaire identified four factors with eigenvalues {\textgreater}2 that explained 67\% of the total variance. Using Cox proportional hazard models, we found no association between any dietary pattern and prostate cancer risk overall (all Ptrend ≥ 0.2). The hazard ratios for quartiles of the dietary pattern scores ranged from 0.87 to 1.14 and all 95\% confidence intervals included 1. The analyses by aggressiveness, Gleason score groups, and age at diagnosis did not show any association (all Ptrend {\textgreater} 0.07). (Cancer Epidemiol Biomarkers Prev 2009;18(11):3126–9)},
  ISSN                     = {1055-9965, 1538-7755},
  Keywords                 = {Cohort Study, Diet, Factor Analysis, Prostate Cancer},
  Language                 = {en}
}

@Article{nagakura_inhibitory_1986,
  Title                    = {Inhibitory effect of 1 alpha,25-dihydroxyvitamin D3 on the growth of the renal carcinoma cell line},
  Author                   = {Nagakura, K and Abe, E and Suda, T and Hayakawa, M and Nakamura, H and Tazaki, H},
  Journal                  = {Kidney international},
