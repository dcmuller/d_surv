  Keywords                 = {Epidemiology, Inclusion probabilities, Nested case-control study, Parametric regression, Partial likelihood, Semiparametric regression, Survival analysis},
  Language                 = {en},
  Url                      = {http://biomet.oxfordjournals.org/content/84/2/379},
  Urldate                  = {2013-07-26}
}

