  Author                   = {Fears, Thomas R. and Guerry, {DuPont} and Pfeiffer, Ruth M. and Sagebiel, Richard W. and Elder, David E. and Halpern, Allan and Holly, Elizabeth A. and Hartge, Patricia and Tucker, Margaret A.},
