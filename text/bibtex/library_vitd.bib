}

@Article{blomberg_jensen_expression_2010,
  Title                    = {Expression of the vitamin D receptor, 25-hydroxylases, 1alpha-hydroxylase and 24-hydroxylase in the human kidney and renal clear cell cancer},
  Author                   = {Blomberg Jensen, Martin and Andersen, Claus BÃ¸gelund and Nielsen, John E and Bagi, Per and JÃ¸rgensen, Anne and Juul, Anders and Leffers, Henrik},
