@Article{schottker_strong_2013,
  Title                    = {Strong associations of 25-hydroxyvitamin D concentrations with all-cause, cardiovascular, cancer, and respiratory disease mortality in a large cohort study},
  Author                   = {Sch\"ottker, Ben and Haug, Ulrike and Schomburg, Lutz and K\"ohrle, Joseph and Perna, Laura and M\"uller, Heiko and Holleczek, Bernd and Brenner, Hermann},
