  ISSN                     = {1097-0258},
  Keywords                 = {Case–cohort design, case–control design, competing risks, full likelihood, nested case–control design, Pseudolikelihood, weighted partial likelihood},
  Language                 = {en},
  Shorttitle               = {Nested case–control data utilized for multiple outcomes},
