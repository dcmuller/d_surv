  ISSN                     = {1097-0258},
  Keywords                 = {cost effectiveness, historical data, population register, study design, Survival analysis, weighted estimation},
  Language                 = {en},
  Url                      = {http://onlinelibrary.wiley.com/doi/10.1002/sim.4494/abstract},
  Urldate                  = {2013-10-31}
}

@Article{samuelsen_psudolikelihood_1997,
