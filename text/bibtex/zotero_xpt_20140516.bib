@Article{zaridze_alcohol_0000,
  Title                    = {Alcohol and cause-specific mortality in Russia: a retrospective case–control study of 48 557 adult deaths},
  Author                   = {Zaridze, David and Brennan, Paul and Boreham, Jillian and Boroda, Alex and Karpov, Rostislav and Lazarev, Alexander and Konobeevskaya, Irina and Igitov, Vladimir and Terechova, Tatiana and Boffetta, Paolo and Peto, Richard},
  Journal                  = {The Lancet},
  Year                     = {0000},

  Month                    = jun,
  Number                   = {9682},
  Pages                    = {2201--2214},
  Volume                   = {373},

  Abstract                 = {{SummaryBackground} Alcohol is an important determinant of the high and fluctuating adult mortality rates in Russia, but cause-specific detail is lacking. Our case–control study investigated the effects of alcohol consumption on male and female cause-specific mortality. Methods In three Russian industrial cities with typical 1990s mortality patterns (Tomsk, Barnaul, Biysk), the addresses of 60 416 residents who had died at ages 15–74 years in 1990–2001 were visited in 2001–05. Family members were present for 50 066 decedents; for 48 557 (97\%), the family gave proxy information on the decedents' past alcohol use and on potentially confounding factors. Cases (n=43 082) were those certified as dying from causes we judged beforehand might be substantially affected by alcohol or tobacco; controls were the other 5475 decedents. Case versus control relative risks ({RRs;} calculated as odds ratios by confounder-adjusted logistic regression) were calculated in ever-drinkers, defining the reference category by two criteria: usual weekly consumption always less than 0·5 half-litre bottles of vodka (or equivalent in total alcohol content) and maximum consumption of spirits in 1 day always less than 0·5 half-litre bottles. Other ever-drinkers were classified by usual weekly consumption into three categories: less than one, one to less than three, and three or more (mean 5·4 [{SD} 1·4]) bottles of vodka or equivalent. Findings In men, the three causes accounting for the most alcohol-associated deaths were accidents and violence ({RR} 5·94, 95\% {CI} 5·35–6·59, in the highest consumption category), alcohol poisoning (21·68, 17·94–26·20), and acute ischaemic heart disease other than myocardial infarction (3·04, 2·73–3·39), which includes some misclassified alcohol poisoning. There were significant excesses of upper aerodigestive tract cancer (3·48, 2·84–4·27) and liver cancer (2·11, 1·64–2·70). Another five disease groups had {RRs} of more than 3·00 in the highest alcohol category: tuberculosis (4·14, 3·44–4·98), pneumonia (3·29, 2·83–3·83), liver disease (6·21, 5·16–7·47), pancreatic disease (6·69, 4·98–9·00), and ill-specified conditions (7·74, 6·48–9·25). Although drinking was less common in women, the {RRs} associated with it were generally more extreme. After correction for reporting errors, alcohol-associated excesses accounted for 52\% of all study deaths at ages 15–54 years (men 8182 [59\%] of 13968, women 1565 [33\%] of 4751) and 18\% of those at 55–74 years (men 3944 [22\%] of 17 536, women 1493 [12\%] of 12 302). Allowance for under-representation of extreme drinkers would further increase alcohol-associated proportions. Large fluctuations in mortality from these ten strongly alcohol-associated causes were the main determinants of recent fluctuations in overall mortality in the study region and in Russia as a whole. Interpretation Alcohol-attributable mortality varies by year; in several recent years, alcohol was a cause of more than half of all Russian deaths at ages 15–54 years. Alcohol accounts for most of the large fluctuations in Russian mortality, and alcohol and tobacco account for the large difference in adult mortality between Russia and western Europe. Funding {UK} Medical Research Council, Cancer Research {UK}, British Heart Foundation, International Agency for Research on Cancer, and European Commission Directorate-General for Research.},
  Doi                      = {10.1016/S0140-6736(09)61034-5},
  File                     = {ScienceDirect Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/8A4G9WK5/Zaridze et al. - Alcohol and cause-specific mortality in Russia a .pdf:application/pdf;ScienceDirect Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/7AD2E5QI/S0140673609610345.html:text/html},
  ISSN                     = {0140-6736},
  Shorttitle               = {Alcohol and cause-specific mortality in Russia},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0140673609610345},
  Urldate                  = {2013-11-14}
}

@Article{zeleniuch-jacquotte_circulating_2010,
  Title                    = {Circulating 25-Hydroxyvitamin D and Risk of Endometrial Cancer Cohort Consortium Vitamin D Pooling Project of Rarer Cancers},
  Author                   = {Zeleniuch-Jacquotte, Anne and Gallicchio, Lisa and Hartmuller, Virginia and Helzlsouer, Kathy J. and {McCullough}, Marjorie L. and Setiawan, V. Wendy and Shu, Xiao-Ou and Weinstein, Stephanie J. and Weiss, Jocelyn M. and Arslan, Alan A. and Vivo, Immaculata De and Gao, Yu-Tang and Hayes, Richard B. and Henderson, Brian E. and Horst, Ronald L. and Koenig, Karen L. and Patel, Alpa V. and Purdue, Mark P. and Snyder, Kirk and Steplowski, Emily and Yu, Kai and Zheng, Wei and Hankinson, Susan E.},
  Journal                  = {American Journal of Epidemiology},
  Year                     = {2010},

  Month                    = jul,
  Note                     = {{PMID:} 20562189},
  Number                   = {1},
  Pages                    = {36--46},
  Volume                   = {172},

  Abstract                 = {A nested case-control study, including 830 cases and 992 controls from 7 cohorts, was conducted to evaluate the association of circulating 25-hydroxyvitamin D (25({OH)D)}, the best indicator of vitamin D status, with risk of endometrial cancer. Matching factors included age at blood donation, date of blood donation, and race. Conditional logistic regression was used in the main analysis. The median concentration of 25({OH)D} was slightly lower in cases (49.4 {nmol/L)} than in controls (50.8 {nmol/L)} (P = 0.08). However, there was no association between 25({OH)D} concentration and disease risk, after adjustment for body mass index. Compared with the 50–{\textless}75 {nmol/L} 25({OH)D} category, the body mass index-adjusted odds ratios and 95\% confidence intervals were 1.08 (95\% confidence interval: 0.73, 1.57) for the {\textless}25 {nmol/L} category and 0.90 (95\% confidence interval: 0.51, 1.58) for the ≥100 {nmol/L} category (Ptrend = 0.99). Similarly null results were observed after further adjustment for other known risk factors and in stratified analyses. Although an effect of circulating 25({OH)D} at high concentrations cannot be ruled out (the highest category of 25({OH)D} was ≥100 {nmol/L}, and for stratified analyses, ≥75 {nmol/L)}, these results do not support a protective role of vitamin D against endometrial cancer.},
  Doi                      = {10.1093/aje/kwq114},
  File                     = {Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/A7R2AK9Q/Zeleniuch-Jacquotte et al. - 2010 - Circulating 25-Hydroxyvitamin D and Risk of Endome.pdf:application/pdf;Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/4DCG4QKP/36.html:text/html},
  ISSN                     = {0002-9262, 1476-6256},
  Keywords                 = {case-control studies, endometrial neoplasms, prospective studies, vitamin D},
  Language                 = {en},
  Url                      = {http://aje.oxfordjournals.org/content/172/1/36},
  Urldate                  = {2013-10-16}
}

@Article{zheng_circulating_2010,
  Title                    = {Circulating 25-Hydroxyvitamin D and Risk of Epithelial Ovarian Cancer Cohort Consortium Vitamin D Pooling Project of Rarer Cancers},
  Author                   = {Zheng, Wei and Danforth, Kim N. and Tworoger, Shelley S. and Goodman, Marc T. and Arslan, Alan A. and Patel, Alpa V. and {McCullough}, Marjorie L. and Weinstein, Stephanie J. and Kolonel, Laurence N. and Purdue, Mark P. and Shu, Xiao-Ou and Snyder, Kirk and Steplowski, Emily and Visvanathan, Kala and Yu, Kai and Zeleniuch-Jacquotte, Anne and Gao, Yu-Tang and Hankinson, Susan E. and Harvey, Chinonye and Hayes, Richard B. and Henderson, Brian E. and Horst, Ronald L. and Helzlsouer, Kathy J.},
  Journal                  = {American Journal of Epidemiology},
  Year                     = {2010},

  Month                    = jul,
  Note                     = {{PMID:} 20562186},
  Number                   = {1},
  Pages                    = {70--80},
  Volume                   = {172},

  Abstract                 = {A role for vitamin D in ovarian cancer etiology is supported by ecologic studies of sunlight exposure, experimental mechanism studies, and some studies of dietary vitamin D intake and genetic polymorphisms in the vitamin D receptor. However, few studies have examined the association of circulating 25-hydroxyvitamin D (25({OH)D)}, an integrated measure of vitamin D status, with ovarian cancer risk. A nested case-control study was conducted among 7 prospective studies to evaluate the circulating 25({OH)D} concentration in relation to epithelial ovarian cancer risk. Logistic regression models were used to estimate odds ratios and 95\% confidence intervals among 516 cases and 770 matched controls. Compared with 25({OH)D} concentrations of 50–{\textless}75 {nmol/L}, no statistically significant associations were observed for {\textless}37.5 (odds ratio ({OR)} = 1.21, 95\% confidence interval ({CI):} 0.87, 1.70), 37.5–{\textless}50 ({OR} = 1.03, 95\% {CI:} 0.75, 1.41), or ≥75 ({OR} = 1.11, 95\% {CI:} 0.79, 1.55) {nmol/L.} Analyses stratified by tumor subtype, age, body mass index, and other variables were generally null but suggested an inverse association between 25({OH)D} and ovarian cancer risk among women with a body mass index of ≥25 kg/m2 (Pinteraction {\textless} 0.01). In conclusion, this large pooled analysis did not support an overall association between circulating 25({OH)D} and ovarian cancer risk, except possibly among overweight women.},
  Doi                      = {10.1093/aje/kwq118},
  File                     = {Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/CVFMMR4K/Zheng et al. - 2010 - Circulating 25-Hydroxyvitamin D and Risk of Epithe.pdf:application/pdf;Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/NX9ESB3X/70.html:text/html},
  ISSN                     = {0002-9262, 1476-6256},
  Keywords                 = {case-control studies, cohort studies, ovarian neoplasms, prospective studies, vitamin D},
  Language                 = {en},
  Url                      = {http://aje.oxfordjournals.org/content/172/1/70},
  Urldate                  = {2013-10-16}
}

@Article{zheng_adopting_2013,
  Title                    = {Adopting nested case–control quota sampling designs for the evaluation of risk markers},
  Author                   = {Zheng, Yingye and Cai, Tianxi and Pepe, Margaret S.},
  Journal                  = {Lifetime Data Analysis},
  Year                     = {2013},

  Month                    = oct,
  Number                   = {4},
  Pages                    = {568--588},
  Volume                   = {19},

  Abstract                 = {Two-phase study methods, in which more detailed or more expensive exposure information is only collected on a sample of individuals with events and a small proportion of other individuals, are expected to play a critical role in biomarker validation research. One major limitation of standard two-phase designs is that they are most conveniently employed with study cohorts in which information on longitudinal follow-up and other potential matching variables is electronically recorded. However for many practical situations, at the sampling stage such information may not be readily available for every potential candidates. Study eligibility needs to be verified by reviewing information from medical charts one by one. In this manuscript, we study in depth a novel study design commonly undertaken in practice that involves sampling until quotas of eligible cases and controls are identified. We propose semiparametric methods to calculate risk distributions and a wide variety of prediction indices when outcomes are censored failure times and data are collected under the quota sampling design. Consistency and asymptotic normality of our estimators are established using empirical process theory. Simulation results indicate that the proposed procedures perform well in finite samples. Application is made to the evaluation of a new risk model for predicting the onset of cardiovascular disease.},
  Doi                      = {10.1007/s10985-013-9270-8},
  File                     = {Lifetime Data Analysis Volume Online First Articles issue [doi 10.1007%2Fs10985-013-9270-8] Yingye Zheng, Tianxi Cai, Margaret S. Pepe -- Adopting nested case–control quota sampling designs for the evaluation of risk markers.pdf:/home/mullerd/Dropbox/Library/zotero/storage/9I5XVT6W/Lifetime Data Analysis Volume Online First Articles issue [doi 10.1007%2Fs10985-013-9270-8] Yingye Zheng, Tianxi Cai, Margaret S. Pepe -- Adopting nested case–control quota sampling designs .pdf:application/pdf;Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/XEGGVNU5/s10985-013-9270-8.html:text/html},
  ISSN                     = {1380-7870, 1572-9249},
  Keywords                 = {Biomarker, Nested case–control study, Operations {Research/Decision} Theory, Prediction accuracy, prognosis, Quality Control, Reliability, Safety and Risk, quota sampling, risk prediction, Statistics for {Business/Economics/Mathematical} {Finance/Insurance}, Statistics for Life Sciences, Medicine, Health Sciences, Statistics, general},
  Language                 = {en},
  Url                      = {http://link.springer.com/article/10.1007/s10985-013-9270-8},
  Urldate                  = {2013-10-31}
}

@Article{zhou_assessment_2013,
  Title                    = {Assessment of biomarkers for risk prediction with nested case–control studies},
  Author                   = {Zhou, Qian M. and Zheng, Yingye and Cai, Tianxi},
  Journal                  = {Clinical Trials},
  Year                     = {2013},

  Month                    = oct,
  Note                     = {{PMID:} 24013405},
  Number                   = {5},
  Pages                    = {677--679},
  Volume                   = {10},

  Abstract                 = {Background Accurate risk prediction plays a key role in disease prevention and disease management; emergence of new biomarkers may lead to an important question about how much improvement in prediction accuracy it would achieve by adding the new markers into the existing risk prediction tools. Purpose In large prospective cohort studies, the standard full-cohort design, requiring marker measurement on the entire cohort, may be infeasible due to cost and low rate of the clinical condition of interest. To overcome such difficulties, nested case–control ({NCC)} studies provide cost-effective alternatives but bring about challenges in statistical analyses due to complex data sets generated. Methods To evaluate prognostic accuracy of a risk model, Cai and Zheng proposed a class of nonparametric inverse probability weighting ({IPW)} estimators for accuracy measures in the time-dependent receiver operating characteristic curve analysis. To accommodate a three-phase {NCC} design in Nurses’ Health Study, we extend the double {IPW} estimators of Cai and Zheng to develop risk prediction models under time-dependent generalized linear models and evaluate the incremental values of new biomarkers and genetic markers. Results Our results suggest that aggregating the information from both the genetic markers and biomarkers substantially improves the accuracy for predicting 5-year and 10-year risks of rheumatoid arthritis. Conclusions Our method provided robust procedures to evaluate the incremental value of new biomarkers allowing for complex sampling designs.},
  Doi                      = {10.1177/1740774513498321},
  File                     = {Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/VHVZW9JK/677.html:text/html},
  ISSN                     = {1740-7745, 1740-7753},
  Language                 = {en},
  Url                      = {http://ctj.sagepub.com/content/10/5/677},
  Urldate                  = {2013-10-31}
}

@Article{_kidney_1986,
  Title                    = {Kidney International - Abstract of article: Inhibitory effect of 1α,25-dihydroxyvitamin D3 on the growth of the renal carcinoma cell line},
  Journal                  = {Kidney Int},
  Year                     = {1986},

  Month                    = apr,
  Number                   = {4},
  Pages                    = {834--840},
  Volume                   = {29},

  Abstract                 = {Inhibitory effect of 1,25-dihydroxyvitamin D3 on the growth of the renal carcinoma cell line. We studied the effect of vitamin D compounds on the growth of the human renal carcinoma cell line ({KU-2)} and discovered a receptor protein specific for the active form of vitamin D3, 1,25-dihydroxyvitamin D3. The {KU-2} cell line was established from a pulmonary metastasis of renal cell carcinoma in a patient with hyperhemoglobinemia. The cells were tumorigenic in nude mice and clonogenic in a soft agar culture. Vitamin D3 derivatives suppressed proliferation of {KU-2} cells in a monolayer culture and also clonogenicity in a soft agar culture dose-dependently. Of the vitamin D3 derivatives tested, 1,25-dihydroxyvitamin D3 was the most potent in inhibiting cell growth, followed successively by {1,24R},25-trihydroxyvitamin D3, 25-hydroxyvitamin D3, 1-hydroxyvitamin D3 and {24R},25-dihydroxyvitamin D3 in that order. Analysis of the cell cycle phase of treated and non-treated {KU-2} cells revealed that the action of 1,25-dihydroxyvitamin D3 was not phase–specific but simply extended the doubling time of the cells. Radioreceptor assay and sucrose density gradient analysis of the cytosol showed that {KU-2} cells contained a {3.2S} receptor protein to which 1, 25-dihydroxyvitamin D3 was specifically bound (Kd = 20.8 4.8 {pM}, Nmax = 87 24 fmole/mg protein, 4000 molecules/cell). On the other hand, the equilibrium dissociation constant of internalization of 1,25-dihydroxyvitamin D3 (Kint) by intact {KU-2} cells was 1.2 {nM} and the internalizing capacity was 33 fmole/8 106 cells (2500 molecules/cell) in the 10\% serum medium, which was the same as that used in the growth study. This Kint value was very close to the half-maximal dose in growth inhibition. Also the affinity of various vitamin D3 derivatives for binding to the cytosol receptor in the {KU-2} cells was closely related to the ability to inhibit growth of the cells. These results indicate that the actions of vitamin D3 derivatives in inhibiting proliferation and clonogenicity of {KU-2} cells are affected by a receptor-mediated mechanism, and that the active form of vitamin D3 may be one of the regulatory factors affecting the proliferation and other biological functions of renal carcinoma cells.},
  File                     = {Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/WS8BMS2R/1986 - Kidney International - Abstract of article Inhibi.pdf:application/pdf;Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/89D6NDQX/ki198674a.html:text/html},
  ISSN                     = {0085-2538},
  Shorttitle               = {Kidney International - Abstract of article},
  Url                      = {http://www.nature.com/ki/journal/v29/n4/abs/ki198674a.html},
  Urldate                  = {2014-01-27}
}

