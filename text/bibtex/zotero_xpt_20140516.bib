  File                     = {Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/E6MPZKBI/Royston and Parmar - 2002 - Flexible parametric proportional-hazards and propo.pdf:application/pdf;Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/BT7T3SAA/abstract.html:text/html},
  ISSN                     = {1097-0258},
  Keywords                 = {parametric models, proportional hazards, proportional odds, splines, Survival analysis},
  Language                 = {en},
  Url                      = {http://onlinelibrary.wiley.com/doi/10.1002/sim.1203/abstract},
  Urldate                  = {2013-10-16}
}

@Article{saarela_nested_2008,
