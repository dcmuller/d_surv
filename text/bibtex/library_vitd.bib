@Article{sorensen_competing_2000,
  Title                    = {Competing risks analysis of the case-cohort design},
  Author                   = {Sørensen, P. and Andersen, P. K.},
  Journal                  = {Biometrika},
  Year                     = {2000},

  Month                    = mar,
  Number                   = {1},
  Pages                    = {49--59},
