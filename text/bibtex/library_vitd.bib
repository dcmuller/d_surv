@Article{prineas_nutrition_1997,
  Title                    = {Nutrition and Other Risk Factors for Renal Cell Carcinoma in Postmenopausal Women},
  Author                   = {Prineas, Ronald and Folsom, Aaron and Zhang, Zhu and Sellers, Thomas and Potter, John},
  Journal                  = {Epidemiology January 1997},
