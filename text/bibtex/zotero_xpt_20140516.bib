  Language                 = {en},
  Shorttitle               = {Calcium and vitamin D supplements and health outcomes},
  Url                      = {http://ajcn.nutrition.org/content/94/4/1144},
  Urldate                  = {2013-10-30}
}

@Article{bolton_interval_2013,
