@Article{pilz_vitamin_2009,
  Title                    = {Vitamin D and mortality in older men and women},
  Author                   = {Pilz, Stefan and Dobnig, Harald and Nijpels, Giel and Heine, Robert J. and Stehouwer, Coen D. A. and Snijder, Marieke B. and Van Dam, Rob M. and Dekker, Jacqueline M.},
