  Journal                  = {American Journal of Epidemiology},
  Year                     = {2010},

  Month                    = jul,
  Note                     = {{PMID:} 20562184},
  Number                   = {1},
  Pages                    = {58--69},
  Volume                   = {172},

  Abstract                 = {Case-control studies generally suggesting an inverse association between sun exposure and non-Hodgkin lymphoma ({NHL)} have led to speculation that vitamin D may protect against lymphomagenesis. To examine this hypothesis, the authors conducted a pooled investigation of circulating 25-hydroxyvitamin D (25({OH)D)} and subsequent {NHL} risk within 10 cohorts participating in the Cohort Consortium Vitamin D Pooling Project of Rarer Cancers. The authors analyzed measurements from 1,353 cases and 1,778 controls using conditional logistic regression and other methods to estimate the association of 25({OH)D} with {NHL.} No clear evidence of association between categories of 25({OH)D} concentration and {NHL} was observed overall (Ptrend = 0.68) or by sex (men, Ptrend = 0.50; women, Ptrend = 0.16). Findings for other measures (continuous log(25({OH)D)}, categories of 25({OH)D} using sex-/cohort-/season-specific quartiles as cutpoints, categories of season-adjusted residuals of predicted 25({OH)D} using quartiles as cutpoints) were generally null, although some measures of increasing 25({OH)D} were suggestive of an increased risk for women. Results from stratified analyses and investigations of histologic subtypes of {NHL} were also null. These findings do not support the hypothesis that elevated circulating 25({OH)D} concentration is associated with a reduced risk of {NHL.} Future research investigating the biologic basis for the {sunlight–NHL} association should consider alternative mechanisms, such as immunologic effects.},
  Doi                      = {10.1093/aje/kwq117},
  File                     = {Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/WCTZEG2H/Purdue et al. - 2010 - Circulating 25-Hydroxyvitamin D and Risk of Non-Ho.pdf:application/pdf;Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/K32KHF3I/58.html:text/html},
  ISSN                     = {0002-9262, 1476-6256},
  Keywords                 = {25-hydroxyvitamin D 2, calcifediol, calcitrol, case-control studies, cohort studies, lymphoma, non-Hodgkin, prospective studies, vitamin D},
  Language                 = {en},
  Url                      = {http://aje.oxfordjournals.org/content/172/1/58},
  Urldate                  = {2013-10-16}
}

@Article{purdue_genome-wide_2011,
  Title                    = {Genome-wide association study of renal cell carcinoma identifies two susceptibility loci on 2p21 and 11q13.3},
  Author                   = {Purdue, Mark P. and Johansson, Mattias and Zelenika, Diana and Toro, Jorge R. and Scelo, Ghislaine and Moore, Lee E. and Prokhortchouk, Egor and Wu, Xifeng and Kiemeney, Lambertus A. and Gaborieau, Valerie and Jacobs, Kevin B. and Chow, Wong-Ho and Zaridze, David and Matveev, Vsevolod and Lubinski, Jan and Trubicka, Joanna and Szeszenia-Dabrowska, Neonila and Lissowska, Jolanta and Rudnai, Péter and Fabianova, Eleonora and Bucur, Alexandru and Bencko, Vladimir and Foretova, Lenka and Janout, Vladimir and Boffetta, Paolo and Colt, Joanne S. and Davis, Faith G. and Schwartz, Kendra L. and Banks, Rosamonde E. and Selby, Peter J. and Harnden, Patricia and Berg, Christine D. and Hsing, Ann W. and Iii, Robert L. Grubb and Boeing, Heiner and Vineis, Paolo and Clavel-Chapelon, Françoise and Palli, Domenico and Tumino, Rosario and Krogh, Vittorio and Panico, Salvatore and Duell, Eric J. and Quirós, José Ramón and Sanchez, Maria-José and Navarro, Carmen and Ardanaz, Eva and Dorronsoro, Miren and Khaw, Kay-Tee and Allen, Naomi E. and Bueno-de-Mesquita, H. Bas and Peeters, Petra H. M. and Trichopoulos, Dimitrios and Linseisen, Jakob and Ljungberg, Börje and Overvad, Kim and Tjønneland, Anne and Romieu, Isabelle and Riboli, Elio and Mukeria, Anush and Shangina, Oxana and Stevens, Victoria L. and Thun, Michael J. and Diver, W. Ryan and Gapstur, Susan M. and Pharoah, Paul D. and Easton, Douglas F. and Albanes, Demetrius and Weinstein, Stephanie J. and Virtamo, Jarmo and Vatten, Lars and Hveem, Kristian and Njølstad, Inger and Tell, Grethe S. and Stoltenberg, Camilla and Kumar, Rajiv and Koppova, Kvetoslava and Cussenot, Olivier and Benhamou, Simone and Oosterwijk, Egbert and Vermeulen, Sita H. and Aben, Katja K. H. and van der Marel, Saskia L. and Ye, Yuanqing and Wood, Christopher G. and Pu, Xia and Mazur, Alexander M. and Boulygina, Eugenia S. and Chekanov, Nikolai N. and Foglio, Mario and Lechner, Doris and Gut, Ivo and Heath, Simon and Blanche, Hélène and Hutchinson, Amy and Thomas, Gilles and Wang, Zhaoming and Yeager, Meredith and Jr, Joseph F. Fraumeni and Skryabin, Konstantin G. and {McKay}, James D. and Rothman, Nathaniel and Chanock, Stephen J. and Lathrop, Mark and Brennan, Paul},
  Journal                  = {Nature Genetics},
