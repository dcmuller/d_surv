@Article{weinstein_serum_2011-1,
  Title                    = {Serum 25-Hydroxyvitamin D and Risks of Colon and Rectal Cancer in Finnish Men},
  Author                   = {Weinstein, Stephanie J. and Yu, Kai and Horst, Ronald L. and Ashby, Jason and Virtamo, Jarmo and Albanes, Demetrius},
  Journal                  = {American Journal of Epidemiology},
  Year                     = {2011},

  Month                    = mar,
  Note                     = {{PMID:} 21248311},
  Number                   = {5},
  Pages                    = {499--508},
  Volume                   = {173},

  Abstract                 = {Prospective investigations of circulating vitamin D concentrations suggest inverse associations with colorectal cancer risk, although inconsistencies remain and few studies have examined the impact of season. The authors conducted a prospective case-control study of 239 colon cancer cases and 192 rectal cancer cases (diagnosed in 1993–2005) and 428 controls matched on age and blood collection date within the Alpha-Tocopherol, Beta-Carotene Cancer Prevention Study, a cohort study of Finnish male smokers. Baseline serum 25-hydroxyvitamin D (25({OH)D)} concentrations were categorized using a priori defined cutpoints of {\textless}25, 25–{\textless}37.5, 37.5–{\textless}50, 50–{\textless}75, and ≥75 {nmol/L} and by season-specific and season-standardized 25({OH)D} quartiles. Conditional logistic regression models yielded multivariate-adjusted odds ratios for the predefined cutpoints of 0.63, 0.91, 0.73, 1.00 (referent), and 1.44 for colon cancer and 0.64, 0.58, 0.84, 1.00, and 0.76 for rectal cancer, respectively (all 95\% confidence intervals included 1.00). Colon cancer risks were significantly elevated for the highest season-specific and season-standardized quartiles versus the lowest quartiles ({OR} = 2.11 (95\% {CI:} 1.20, 3.69) and {OR} = 1.88 (95\% {CI:} 1.07, 3.28), respectively), while rectal cancer risk estimates were null. These results provide no evidence to support an inverse association between vitamin D status and colon or rectal cancer risk; instead, they suggest a positive association for colon cancer.},
  Doi                      = {10.1093/aje/kwq398},
  File                     = {Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/52T85QAM/Weinstein et al. - 2011 - Serum 25-Hydroxyvitamin D and Risks of Colon and R.pdf:application/pdf;Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/PBTCAGZM/499.html:text/html},
  ISSN                     = {0002-9262, 1476-6256},
  Keywords                 = {cohort studies, colorectal neoplasms, prospective studies, vitamin D},
  Language                 = {en},
  Url                      = {http://aje.oxfordjournals.org/content/173/5/499},
  Urldate                  = {2013-10-16}
}

@Article{weinstein_serum_2011,
  Title                    = {Serum 25-Hydroxyvitamin D and Risk of Lung Cancer in Male Smokers: A Nested Case-Control Study},
  Author                   = {Weinstein, Stephanie J. and Yu, Kai and Horst, Ronald L. and Parisi, Dominick and Virtamo, Jarmo and Albanes, Demetrius},
  Journal                  = {{PLoS} {ONE}},
  Year                     = {2011},

  Month                    = jun,
  Number                   = {6},
  Pages                    = {e20796},
  Volume                   = {6},

  Abstract                 = {Background A role for vitamin D in cancer risk reduction has been hypothesized, but few data exist for lung cancer. We investigated the relationship between vitamin D status, using circulating 25-hydroxyvitamin D [25({OH)D]}, and lung cancer risk in a nested case-control study within the Alpha-Tocopherol, Beta-Carotene Cancer Prevention Study of Finnish male smokers.
Methods Lung cancer cases (n = 500) were randomly selected based on month of blood collection, and 500 controls were matched to them based on age and blood collection date. Odds ratios ({ORs)} and 95\% confidence intervals ({CIs)} were calculated using multivariate-adjusted conditional logistic regression. To account for seasonal variation in 25({OH)D} concentrations, season-specific and season-standardized quintiles of 25({OH)D} were examined, and models were also stratified on season of blood collection (darker season = {November–April} and sunnier season = {May–October).} Pre-determined, clinically-defined cutpoints for 25({OH)D} and 25({OH)D} as a continuous measure were also examined.
Results Overall, 25({OH)D} was not associated with lung cancer. Risks were 1.08 (95\% {CI} 0.67–1.75) and 0.83 (95\% {CI} 0.53–1.31) in the highest vs. lowest season-specific and season-standardized quintiles of 25({OH)D}, respectively, and 0.91 (95\% {CI} 0.48–1.72) for the ≥75 vs. {\textless}25 {nmol/L} clinical categories. Inverse associations were, however, suggested for subjects with blood collections from {November–April}, with {ORs} of 0.77 (95\% {CI} 0.41–1.45, p-trend = 0.05) and 0.65 (95\% {CI} 0.37–1.14, p-trend = 0.07) in the highest vs. lowest season-specific and season-standardized quintiles of 25({OH)D}, respectively, and 0.61 (95\% {CI} 0.24–1.52, p-trend = 0.01) for ≥75 vs. {\textless}25 {nmol/L.} We also found 11\% lower risk for a 10 {nmol/L} increase in 25({OH)D} in the darker season based on the continuous measure ({OR} = 0.89, 95\% {CI} 0.81–0.98, p = 0.02).
Conclusion In this prospective study of male smokers, circulating 25({OH)D} was not associated with lung cancer risk overall, although inverse associations were suggested among those whose blood was drawn during darker months.},
  Doi                      = {10.1371/journal.pone.0020796},
  File                     = {PLoS Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/QRDNDPFX/Weinstein et al. - 2011 - Serum 25-Hydroxyvitamin D and Risk of Lung Cancer .pdf:application/pdf;PLoS Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/9B2V3KKS/infodoi10.1371journal.pone.html:text/html},
  Shorttitle               = {Serum 25-Hydroxyvitamin D and Risk of Lung Cancer in Male Smokers},
  Url                      = {http://dx.doi.org/10.1371/journal.pone.0020796},
  Urldate                  = {2013-10-16}
}

@Article{van_der_wielen_serum_1995,
  Title                    = {Serum vitamin D concentrations among elderly people in Europe},
  Author                   = {van der Wielen, {R.P.J.} and de Groot, {L.C.P.G.M.} and van Staveren, {W.A.} and Löwik, {M.R.H.} and van den Berg, H. and Haller, J. and Moreiras, O.},
  Journal                  = {The Lancet},
  Year                     = {1995},

  Month                    = jul,
  Number                   = {8969},
  Pages                    = {207--210},
  Volume                   = {346},

  Abstract                 = {Vitamin D status decreases with age, mainly as a result of restricted sunlight exposure, reduced capacity of the skin to produce vitamin D, and reduced dietary vitamin D intake. We measured wintertime serum 25-hydroxyvitamin D [25({OH)D]} concentrations in 824 elderly people from 11 European countries. 36\% of men and 47\% of women had 25({OH)D} concentrations below 30 {nmol/L.} Users of vitamin D supplements and/or sunlamps had higher 25({OH)D} (median 54 {nmol/L)} than non users (median 31 {nmol/L).} Surprisingly, lowest mean 25({OH)D} concentrations were seen in southern European countries. Low 25({OH)D} concentrations could largely be explained by attitudes towards sunlight exposure and factors of physical health status, after exclusion of users of vitamin D supplements or sunlamps. Problems with daily living activities and wearing clothes with long sleeves during periods of sunshine were strong predictors of low wintertime serum 25({OH)D} concentrations. These findings show that free-living elderly Europeans, regardless of geographical location, are at substantial risk of inadequate vitamin D status during winter and that dietary enrichment or supplementation with vitamin D should be seriously considered during this season.},
  Doi                      = {10.1016/S0140-6736(95)91266-5},
  File                     = {ScienceDirect Full Text PDF:/home/mullerd/Dropbox/Library/zotero/storage/IHKEMSC2/van der Wielen et al. - 1995 - Serum vitamin D concentrations among elderly peopl.pdf:application/pdf;ScienceDirect Snapshot:/home/mullerd/Dropbox/Library/zotero/storage/8N6DPUQC/S0140673695912665.html:text/html},
  ISSN                     = {0140-6736},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0140673695912665},
  Urldate                  = {2013-11-06}
}

