@Article{chen_meta-analysis_2010,
  Title                    = {Meta-analysis of vitamin D, calcium and the prevention of breast cancer},
  Author                   = {Chen, Peizhan and Hu, Pingting and Xie, Dong and Qin, Ying and Wang, Fudi and Wang, Hui},
  Journal                  = {Breast cancer research and treatment},
  Year                     = {2010},

